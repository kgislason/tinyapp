<!DOCTYPE html>
<html lang="en">
<head>
  <%- include('../partials/head'); %>
</head>
<body class="container">

<header>
  <%- include('../partials/header'); %>
</header>

<main class="py-5">
  <h1>My URLs</h1>
  <table class="table table-dark">
    <thead>
      <tr>
        <th scope="col">Short URL ID</th>
        <th scope="col">Long URL</th>
      </tr>
    </thead>
    <tbody>
      <% for (let id in urls) { %>
        <tr>
          <td><%= id %></td>
          <td><%= urls[id] %></td>
        </tr>
      <% } %>
    </tbody>
  </table>
</main>

<footer>
  <%- include('../partials/footer'); %>
</footer>

<%- include('../partials/scripts'); %>
</body>
</html>